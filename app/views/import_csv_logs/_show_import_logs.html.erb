<h4>Import Log</h4>
<% if import_csv_log.present? %>
<label>Status: </label><%= import_csv_log.status %>
<table>
  <tr>
    <td>Line no</td>
    <td>Status</td>
    <td>Description</td>
  </tr>
  <tr>
<% if import_csv_log.logs.present? %>
  <% import_csv_log.logs.each do |log| %>
    <td><%= log[:line_no] %></td>
    <td><%= log[:status] %></td>
    <td>
      <% if log[:description].kind_of?(Array) %>
        <ul>
        <% log[:description].each do |error| %>
          <li><%= error %></li>
        <% end %>
      <% else %>
        <%= log[:description] %>
      <% end %>
      <%=  %>
    </td>
  <% end %>
<% end %>
</table>
<% else %>
  Nothing to show
<% end %>
<%= link_to 'Back', data_center_path, class: 'button_css' %>